package arrayexample;
class Student{
	private String rollNo;
	private String name;
	private String mail;
	private int age;
	
	//setter method
	public void setData(String x,String y,String z,int a) {
		rollNo=x;
		name=y;
		mail=z;
		age=a;
	}
	 
	//getter method
	public String getRollNo() {
		return rollNo;
	}
	public String getName() {
		return name;
	}
	public String getMail() {
		return mail;
	}
	public int getAge() {
		return age;
	}
}
public class Encapsulationexample {

	public static void main(String[] args) {
		Student s1=new Student();
		Student s2=new Student();
		s1.setData("A55","Vineela","gandhamvineela@gmail.com",19);
		s2.setData("A102", "Sri", "thanjavursri7@gamil.com", 20);
		System.out.println("The student roll no:"+s1.getRollNo());
		System.out.println("The student name is :"+s1.getName());
		System.out.println("The student mail is:"+s1.getMail());
		System.out.println("The student age is :"+s1.getAge());
		System.out.println("The 2nd student roll no:"+s2.getRollNo());
		System.out.println("The 2nd student name :"+s2.getName());
		System.out.println("The 2nd student mail:"+s2.getMail());
		System.out.println("The 2nd student age is:"+s2.getAge());
		
	}

}
